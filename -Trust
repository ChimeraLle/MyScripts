-- Variables

local CurrentWalkSpeed = Sc.CharacterWalkSpeed
local CurrentJumpPower = Sc.CharacterJumpPower
local CurrentGravity = workspace.Gravity

local plr = plrs.LocalPlayer

local SavedWalkSpeed = CurrentWalkSpeed
local SavedJumpPower = CurrentJumpPower

-- LocalPlayer Changers

local function ChangeSpeed()
	
	local Char = plr.Character or plr.CharacterAdded:Wait()
	local hum = Char:FindFirstChildWhichIsA("Humanoid")

	if hum then
		hum.WalkSpeed = SavedWalkSpeed
	end
	
end

local function ChangePower()

	local Char = plr.Character or plr.CharacterAdded:Wait()
	local hum = Char:FindFirstChildWhichIsA("Humanoid")

	if hum then
		hum.JumpPower = SavedJumpPower
	end

end

-- Actions

local function ButtonAction(ButtonName)
	
end

local function ToggleAction(ToggleName, Bool)
	
end

local function InputAction(InputName, InputValue)
	
end

local function SliderAction(SliderName, SlideValue)
	if SliderName == "Gravity" then
		
		local ValueMultiplayer = SlideValue * 10

		local function Change()
			workspace.Gravity = CurrentGravity * ValueMultiplayer
		end

		Change()
		
	elseif SliderName == "WalkSpeed" then
		
		SavedWalkSpeed = ((SlideValue * 10) * CurrentWalkSpeed) + CurrentWalkSpeed
		
		ChangeSpeed()
		
	elseif SliderName == "JumpPower" then

		SavedJumpPower = ((SlideValue * 10) * CurrentJumpPower) + CurrentJumpPower

		ChangePower()
		
	end
end

-- Died

plr.CharacterAdded:Connect(function()
	ChangeSpeed()
	ChangePower()
end)

-- Base Action

for i, v in ipairs(ScrollingFrame:GetChildren()) do
	if v:IsA("Frame") and v.Name ~= "ZZZ_Space" then

		local Actions = v:FindFirstChild("SectionActions")
		if Actions then
			
			for i, v in ipairs(Actions:GetChildren()) do
				if v:IsA("TextButton") then
					
					local ActionMode = ""
					
					if v:FindFirstChild("Slider") then
						ActionMode = "Slider"
					elseif v:FindFirstChild("Small") then
						ActionMode = "Toggle"
					elseif v:FindFirstChild("Input") then
						ActionMode = "Input"
					elseif v:FindFirstChild("Button") then
						ActionMode = "Button"
					else
						ActionMode = "None"
					end
					
					if ActionMode ~= "None" then
						if ActionMode == "Slider" then
							
							local Slider = v:WaitForChild("Slider")
							local BackSlide = Slider:FindFirstChild("Frame")
							local SlideFrame = BackSlide:FindFirstChild("Frame")
							
							if SlideFrame then
								
								local Slide = false
								
								v.MouseMoved:Connect(function(x, y)
									
									if Slide then
										
										local StartPos = BackSlide.AbsolutePosition.X
										local EndPos = (BackSlide.AbsolutePosition.X + BackSlide.AbsoluteSize.X)
										
										if x >= StartPos then
											if x <= EndPos then
												
												local CurrentPos = x - StartPos
												local MaxPos = EndPos - StartPos
												
												local OffsetToScale = (CurrentPos / MaxPos)
												
												SlideFrame.Size = UDim2.new(OffsetToScale,0,0,10)
												
												SliderAction(v.Name, OffsetToScale)
												
											else
												SlideFrame.Size = UDim2.new(1,0,0,10)
												SliderAction(v.Name, 1)
											end
										else
											SlideFrame.Size = UDim2.new(0,0,0,10)
											SliderAction(v.Name, 0)
										end
										
									end
									
								end)
								
								v.MouseButton1Down:Connect(function()
									Slide = true
								end)
								
								v.MouseButton1Up:Connect(function()
									Slide = false
								end)
								
								v.MouseLeave:Connect(function()
									Slide = false
								end)
								
							end
							
						elseif ActionMode == "Toggle" then
							
							local Small = v:WaitForChild("Small")
							local Icon = Small:FindFirstChild("Icon")
							
							local Val = false
							
							if Small then
								
								v.MouseButton1Down:Connect(function()
									if Val == false then
										Val = true
										Icon.BackgroundColor3 = Color3.new(0,1,0)
										ToggleAction(v.Name)
									else
										Val = false
										Icon.BackgroundColor3 = Color3.new(1,0,0)
									end
								end)
								
							end
							
						elseif ActionMode == "Input" then
							
							local Input = v:WaitForChild("Input")
							local Tb = Input:FindFirstChild("TextBox")
							
							if Tb then
								Tb:GetPropertyChangedSignal("Text"):Connect(function()
									
									InputAction(v.Name, Tb.Text)
									
								end)
							end
							
						elseif ActionMode == "Button" then
							
							v.MouseButton1Down:Connect(function()
								
								ButtonAction(v.Name)
								
							end)
							
						end
					end
				end
			end
		end
	end
end
