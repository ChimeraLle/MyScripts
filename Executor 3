	Execute.BorderSizePixel = 0
	Execute.Size = UDim2.new(0.200000003, 0, 0.600000024, 0)
	Execute.Image = "rbxassetid://11523543"

	Execute_2.Name = "Execute"
	Execute_2.Parent = Execute
	Execute_2.BackgroundColor3 = Color3.fromRGB(193, 182, 181)
	Execute_2.BackgroundTransparency = 1.000
	Execute_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
