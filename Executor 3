local Rs = game:GetService("ReplicatedStorage")

if not Rs:FindFirstChild("Executor_By:ChimeraLle") then
	local new = Instance.new("Folder",Rs)
	new.Name = "Executor_By:ChimeraLle"
	
	local ScreenGui = Instance.new("ScreenGui")
	local ImageLabel = Instance.new("ImageLabel")
	local UICorner = Instance.new("UICorner")
	local UIStroke = Instance.new("UIStroke")
	local Buttons = Instance.new("Frame")
	local UIListLayout = Instance.new("UIListLayout")
	local Execute = Instance.new("ImageLabel")
	local Execute_2 = Instance.new("TextButton")
	local UITextSizeConstraint = Instance.new("UITextSizeConstraint")
	local Clear = Instance.new("ImageLabel")
	local Clear_2 = Instance.new("TextButton")
	local UITextSizeConstraint_2 = Instance.new("UITextSizeConstraint")
	local Text = Instance.new("ScrollingFrame")
	local Text_2 = Instance.new("TextBox")
	local UIPadding = Instance.new("UIPadding")
	
	ScreenGui.Parent = game.CoreGui
	ScreenGui.Enabled = true
	ScreenGui.ResetOnSpawn = false

	ImageLabel.Parent = ScreenGui
	ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
