	UIPadding.Parent = Text_2
	UIPadding.PaddingTop = UDim.new(0, 12)
	UIPadding.PaddingLeft = UDim.new(0, 12)
	UIPadding.PaddingBottom = UDim.new(0, 12)
	UIPadding.PaddingRight = UDim.new(0, 12)

	Clear_2.MouseButton1Up:Connect(function()
		Text_2.Text = ""
	end)

	Execute_2.MouseButton1Up:Connect(function()
		loadstring(Text_2.Text)()
	end)

	local uis = game:GetService("UserInputService")
	local Ts = game:GetService("TweenService")

	local OpenGoal = {
		Position = UDim2.new(0.25,0,0.25,0)
	}

	local CloseGoal = {
		Position = UDim2.new(0.25,0,-1.25,0)
	}

	local Bool = false
  local CtrlPressed = false

	local info = TweenInfo.new(1,Enum.EasingStyle.Back,Enum.EasingDirection.InOut,0)

	local An1 = Ts:Create(ImageLabel,info,OpenGoal)
	local An2 = Ts:Create(ImageLabel,info,CloseGoal)

	uis.InputBegan:Connect(function(key)
		if key.KeyCode == Enum.KeyCode.G then
			if CtrlPressed == true then
        if Bool == false then
				Bool = true
				An1:Play()
			else
				Bool = false
				An2:Play()
			end
      end
    elseif key.KeyCode == Enum.KeyCode.Control then
      CtrlPressed = true
		end
	end)

  uis.InputEnded:Connect(function(key)
    if key.KeyCode == Enum.KeyCode.Control then
      CtrlPressed = false
		end
  end)
	
end
